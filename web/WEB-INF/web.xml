<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>uploads</servlet-name>
        <servlet-class>upload.uploads</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>encryption</servlet-name>
        <servlet-class>pack.encryption</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadnew</servlet-name>
        <servlet-class>network.uploadnew</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>network.download</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download_re</servlet-name>
        <servlet-class>network.download_re</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploads</servlet-name>
        <url-pattern>/uploads</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>encryption</servlet-name>
        <url-pattern>/encryption</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadnew</servlet-name>
        <url-pattern>/uploadnew</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download_re</servlet-name>
        <url-pattern>/download_re</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
